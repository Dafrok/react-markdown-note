*:not(input, textarea)
  user-select none
*
  box-sizing border-box

body
  padding 0
  margin 0
  font-family PingFang SC, Lantinghei SC, Microsoft Yahei, Hiragino Sans GB, Microsoft Sans Serif, WenQuanYi Micro Hei, sans

.status-bar
  position fixed
  left 0
  right 0
  top 0
  height 64px
  z-index 1
.content
  padding-top 64px
.navigator
  a
    //text-decoration none
    //color inherit
    width 100%
    height 100%
    display block
